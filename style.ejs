  }

  .logo {
    height: 100%;
    width: 15%;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-right: 20%;
  }
  .logo a {
    position: fixed;
    font-size: 28px;
    font-weight: 300;
    color: rgb(41, 2, 65);
    text-decoration: none;
  }
  .first-letter {
    color: rgb(3, 124, 90);
  }
  .link {
    display: flex;
    justify-content: space-between;
    width: 65%;
  }
  .link a {
    font-size: 18px;
    text-decoration: none;
    color: rgb(41, 2, 65);
  }
  .link a:hover {
    transition: all 500ms ease-in-out;
    border-bottom: 2px solid #3a58bd ;
    padding-bottom: 10px;
  }
  .nav-link i:hover {
    color: rgb(3, 124, 90);
  }
  .line {
    height: 20px;
    width: 2px;
    border-right: 2px solid rgb(161, 14, 253);
  }
  .social-media {
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: 13%;
    margin-right: 10%;
  }
  .social-link {
    color: rgb(47, 5, 65);
    font-size: 18px;
    transition: all 100ms ease-in-out;
    cursor: pointer;
  }
  .social-link:hover {
    color: rgb(3, 124, 90);
    transform: scale(1.2);
  }
  @media (max-width: 1038px) {
    .link a {
      font-size: 1.2rem;
    }
    .logo {
      width: 10%;
      margin-right: 10%;
    }
    .logo a {
      width: 145px;
      height: 32px;
      font-size: 20px;
    }
  }
  @media (max-width: 700px) {
